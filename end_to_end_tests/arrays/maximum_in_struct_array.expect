#!/usr/bin/expect

# Scenario
#
#    Assert the programming solution for "Maximum In Struct Array" problem
#    computes expected answer for given input. 

set timeout -1

set CATEGORY_DIR "programs/arrays"

set PROGRAM_FILE "maximum_in_struct_array.py"

set PROGRAM_PATH "${CATEGORY_DIR}/${PROGRAM_FILE}"

set NUMBER_OF_TESTCASES "2"

set NUMBER_OF_COMBINATIONS_FIRST "2"

set FEETS_INCHES_FIRST "1221"

# 1221 = (1 feet and 2 heights), (2 feet and 1 height)

set EXPECTED_MAXIMUM_HEIGHT_FIRST "25"

set NUMBER_OF_COMBINATIONS_SECOND "4"

set FEETS_INCHES_SECOND "35795655"

# 3579.. = (3 feet, 7 inches) and (7 feet, 9 inches)...

set EXPECTED_MAXIMUM_HEIGHT_SECOND "93"

spawn python $PROGRAM_PATH

send -- "$NUMBER_OF_TESTCASES\n"

# First input values of Feet and Inches
#---------------------------------------

send -- "$NUMBER_OF_COMBINATIONS_FIRST\n"

send -- "$FEETS_INCHES_FIRST\n"

expect "$EXPECTED_MAXIMUM_HEIGHT_FIRST"

# Second input values of Feet and Inches
# --------------------------------------

send -- "$NUMBER_OF_COMBINATIONS_SECOND\n"

send -- "$FEETS_INCHES_SECOND\n"

expect "$EXPECTED_MAXIMUM_HEIGHT_SECOND"
